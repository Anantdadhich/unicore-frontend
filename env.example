# Environment Variables for UniCore Protocol

# Google Gemini API Key for AI chat assistant
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key_here

# 1inch API Key for swap functionality
ONEINCH_API_KEY=your_1inch_api_key_here

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Contract addresses (update after deployment)
NEXT_PUBLIC_UNICORE_SWAP_ADDRESS=
NEXT_PUBLIC_SOLVER_NETWORK_ADDRESS=
NEXT_PUBLIC_ZK_VERIFIER_ADDRESS=

# RPC URLs (optional - will use public RPCs if not provided)
NEXT_PUBLIC_ETHEREUM_RPC_URL=
NEXT_PUBLIC_POLYGON_RPC_URL=
NEXT_PUBLIC_ARBITRUM_RPC_URL=
NEXT_PUBLIC_OPTIMISM_RPC_URL=
